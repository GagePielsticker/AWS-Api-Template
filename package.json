{
  "name": "aws-api-template",
  "version": "1.0.0",
  "description": "A template for an AWS based serverless api. This utilizes lambda, api-gateway, WAFv2 and other various services to build a scalable and highly-available api. Deployment of all infrastructure fully automated via serverless framework :)",
  "main": "index.js",
  "scripts": {
    "dev": "serverless offline",
    "test": "jest --coverage && coveralls < coverage/lcov.info",
    "build": "serverless package",
    "deploy": "serverless deploy --stage dev --region us-east-1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GagePielsticker/AWS-Api-Template.git"
  },
  "keywords": [
    "aws",
    "serverless",
    "api",
    "service",
    "template",
    "boilerplate"
  ],
  "author": "James Gage Pielsticker",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/GagePielsticker/AWS-Api-Template/issues"
  },
  "homepage": "https://github.com/GagePielsticker/AWS-Api-Template#readme",
  "devDependencies": {
    "coveralls": "^3.1.1",
    "jest": "^29.5.0",
    "serverless": "^3.30.0",
    "serverless-associate-waf": "^1.2.1",
    "serverless-deployment-bucket": "^1.6.0",
    "serverless-offline": "^12.0.4",
    "standard": "^17.0.0"
  }
}
